{
    "name": "radicalloop/smtpapi",
    "description": "Build SendGrid X-SMTPAPI headers in PHP.",
    "version": "0.6.1",
    "homepage": "http://github.com/radicalloop/smtpapi-php",
    "license": "MIT",
    "keywords": ["SendGrid", "sendgrid", "email", "send", "grid", "smtpapi", "smtp", "api", "xsmtp", "X-SMTP"],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.* || 6.5.*"
    },
    "suggest": {
        "swiftmailer/swiftmailer": "^5.4 - Needed to run the example scripts"
    },
    "replace": {
        "radicalloop/smtpapi-php": "*"
    },
    "type": "library",
    "autoload": {
        "psr-0": {"Smtpapi": "lib/"}
    }
}
